{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "~~": ["."],
      "~~/*": ["./*"],
      "@@": ["."],
      "@@/*": ["./*"],
      "~": ["."],
      "~/*": ["./*"],
      "@": ["."],
      "@/*": ["./*"],
      "assets": ["./assets"],
      "assets/*": ["./assets/*"],
      "public": ["./public"],
      "public/*": ["./public/*"],
      "#pages": ["./node_modules/nuxt/dist/pages/runtime"],
      "#pages/*": ["./node_modules/nuxt/dist/pages/runtime/*"],
      "#types": ["./types"],
      "#types/*": ["./types/*"],
      "#stores": ["./stores"],
      "#stores/*": ["./stores/*"],
      "#app": ["./node_modules/nuxt/dist/app"],
      "#app/*": ["./node_modules/nuxt/dist/app/*"],
      "#imports": [".nuxt/imports"],
      "#build": [".nuxt"],
      "#build/*": [".nuxt/*"],
      "#components": [".nuxt/components"],
      "#fake-db": ["./fake-db"],
      "#fake-db/*": ["./fake-db/*"],
      "#api": ["./api"],
      "#api/*": ["./api/*"]
    }
  },
  "exclude": ["./node_modules"],
  "include": ["./**/*.ts", "./nuxt-env.d.ts"]
}
